      "\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@test/(.*)": "<rootDir>/test/$1"
    },
    "testRegex": "(/test/src/.*|(\\.|/)(test|spec))\\.ts$",
    "coveragePathIgnorePatterns": [
      "dist",
      "/node_modules/",
      "test"
    ]
  },
